        <activity android:name=".ViewReservation" />
        <activity android:name=".initReservation" />
        <activity android:name=".DetailizeReservation" />
        <activity android:name=".DetailizeNews" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>